    "@angular/platform-server": "^19.2.8",
    "@angular/router": "^19.2.8",
    "@stomp/rx-stomp": "^2.0.0",
    "express": "^4.18.2",
    "front": "file:",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.1",
    "zone.js": "~0.15.0"
  },
